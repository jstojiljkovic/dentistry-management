<div class="grph-paper">
    <div class="grph-chart-number-container">
        <div class="col-3">
            <div class="grph-chart-number-icon">
                <DentistryManagement.Client.Icons.Graph />
            </div>
        </div>
        <div class="col-9">
            <h6 class="grph-chart-number-h6">Overall earnings</h6>
            <h5 class="grph-chart-number-h5">@OverallEarnings</h5>
        </div>
    </div>
    <div class="grph-chart-number-footer">
        <p>All treatment history's earnings your dentists have made</p>
    </div>
</div>

@code {
    [Parameter]
    public string OverallEarnings { get; set; }
}
